// out: Main.sass, sourcemap: false, compress: true

*
  position relative
  height auto

.clear-float
  clear both

h1, h2, h3, h4, h5, h6, p
  margin 0
  padding 0
  line-height 1

$black = #000
$white = #fff
$off-white = #f1f2f2
$clear-white = rgba(255,255,255,.3)
$dark-grey = #333333
$lgt-grey = #444444

a,
a:link,
a:visited,
a:hover,
a:active
  color inherit
  text-decoration none

h4,h5,h6,p,ol,ul
  line-height 1.3

h1
  font-size 200%
  font-weight 400

p
  a,
  a:link,
  a:visited,
  a:hover,
  a:active
    color inherit
    text-decoration underline

img
  max-width 100%

html
  -webkit-font-smoothing: antialiased;

html, body, #root
  margin 0 auto
  padding 0
  width 100%
  height 100%
  font-size 14px
  letter-spacing -.5px
  font-weight 400
  font-family 'Lato', sans-serif
  color $dark-grey

	/*
  	Flaticon icon font: Flaticon
  	Creation date: 24/04/2016 06:02
  	*/


//FONT IMPORTS

@font-face {
  font-family: "Flaticon";
  src: url("./fonts/icon-font/font/Flaticon.eot");
  src: url("./fonts/icon-font/font/Flaticon.eot?#iefix") format("embedded-opentype"),
       url("./fonts/icon-font/font/Flaticon.woff") format("woff"),
       url("./fonts/icon-font/font/Flaticon.ttf") format("truetype"),
       url("./fonts/icon-font/font/Flaticon.svg#Flaticon") format("svg");
  font-weight: normal;
  font-style: normal;
}

@media screen and (-webkit-min-device-pixel-ratio:0) {
  @font-face {
    font-family: "Flaticon";
    src: url("./fonts/icon-font/font/Flaticon.svg#Flaticon") format("svg");
  }
}

[class^="flaticon-"]:before, [class*=" flaticon-"]:before,
[class^="flaticon-"]:after, [class*=" flaticon-"]:after {
  font-family: Flaticon;
        font-size: 20px;
font-style: normal;
}

.flaticon-bars:before { content: "\f100"; }
.flaticon-circle:before { content: "\f101"; }
.flaticon-envelope:before { content: "\f102"; }
.flaticon-food:before { content: "\f103"; }
.flaticon-gps:before { content: "\f104"; }
.flaticon-technology:before { content: "\f105"; }
